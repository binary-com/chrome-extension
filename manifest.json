{
  "manifest_version": 2,

  "name": "Binary.com websocket extension",
  "description": "This extension enables you to track changes to your ...",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "16":  "icons/favicon-16x16.png",
      "32":  "icons/favicon-32x32.png",
      "96":  "icons/favicon-96x96.png",
      "160": "icons/favicon-160x160.png",
      "192": "icons/favicon-192x192.png"
    },
    "default_popup": "popup.html",
    "default_title": "Binary.com"
  },
  "permissions": [
    "activeTab",
    "notifications",
    "https://ajax.googleapis.com/"
  ]
}